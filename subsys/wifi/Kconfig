menu "WiFi"

config WIFIMGR
        bool "WiFi Manager"
        help
		Enable the WiFi Manager.
		By default, WiFi manger is build as only a library.

if WIFIMGR

config WIFIMGR_CLI
	bool "WiFi Manager Client"
	default y
	help
		A client that can interact with WiFi manger,
		which can be ran from shell.

endif # WiFi

endmenu
